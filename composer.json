{
  "name": "xing/minisite",
  "description": "Minisite library for Durpal",
  "keywords": [
    "minisite",
    "mini site",
    "drupal"
  ],
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Joseph Zhao",
      "email": "joseph.zhao@xing.net.au"
    },
    {
      "name": "Jason Guo",
      "email": "jason.guo@xing.net.au"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "symfony/dom-crawler": "^3.2",
    "alchemy/zippy": "^0.4.5"
  },
  "autoload": {
    "files": [
      "src/functions_include.php"
    ],
    "psr-4": {
      "minisite\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
